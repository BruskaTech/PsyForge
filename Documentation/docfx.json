{
  "metadata": [
    {
      "src": [
        {
          "src": "../Runtime",
          "files": [
            "**/**.cs"
          ],
          "exclude": [
            "**/Plugins/**",
            "**/StandaloneFileBrowser/**",
            "**/TextMeshPro/**"
          ]
        }
      ],
      "dest": "api",
      "force": true,
      "globalNamespaceId": "Global",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.md",
          "api/**.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "sitemap": {
      "baseUrl": "https://bruskatech.github.io/UnityEPL",
      "changefreq": "weekly",
      "fileOptions": {
        "api/*": {
          "changefreq": "daily"
        }   
      }   
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}